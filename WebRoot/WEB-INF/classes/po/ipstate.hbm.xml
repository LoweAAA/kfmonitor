<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="po.Ipstate" table="ipstate">
		<id name="idipstate" column="idipstate">
			<generator class="increment"/>
		</id>
		<property name="ip" column="ip"/>
		<property name="state" column="state"/>
		<property name="gname" column="gname"/>
		<property name="gzone" column="gzone"/>
		<property name="zserver" column="zserver"/>
		<property name="port" column="port"/>
	</class>
	
</hibernate-mapping>